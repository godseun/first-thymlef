_BT.CategSearch = {
    getTargetDepth: function(depth) {
        var depthObj = eval('depth' + depth);
        return depthObj
    },
    getTargetName: function(key) {
        for (var x = 1; x < 5; x++) {
            var list = eval('depth' + x);
            for (var prop in list) {
                if (key == list[prop].C_CD) {
                    return list[prop].C_NM;
                    break
                }
            }
        }
    },
    getTargetInfo: function(key) {
        var result = {};
        for (var d4 = 4; d4 > 0; d4--) {
            var list = eval('depth' + d4);
            for (var prop in list) {
                if (key == list[prop].C_CD) {
                    T_CD = list[prop].T_CD;
                    P_CD = list[prop].P_CD;
                    result = list[prop]
                }
            }
        }
        return result
    },
    getTargetTree: function(key) {
        var result = {};
        for (var d4 = 4; d4 > 0; d4--) {
            var list = eval('depth' + d4);
            for (var prop in list) {
                if (key == list[prop].C_CD) {
                    T_CD = list[prop].T_CD;
                    P_CD = list[prop].P_CD;
                    result[d4] = list[prop];
                    for (var d3 = d4; d3 > 0; d3--) {
                        var list = eval('depth' + d3);
                        for (var prop in list) {
                            if (P_CD == list[prop].C_CD) {
                                T_CD = list[prop].T_CD;
                                P_CD = list[prop].P_CD;
                                result[d3] = list[prop];
                                for (var d2 = d3; d2 > 0; d2--) {
                                    var list = eval('depth' + d2);
                                    for (var prop in list) {
                                        if (P_CD == list[prop].C_CD) {
                                            T_CD = list[prop].T_CD;
                                            P_CD = list[prop].P_CD;
                                            result[d2] = list[prop];
                                            for (var d1 = d2; d1 > 0; d1--) {
                                                var list = eval('depth' + d1);
                                                for (var prop in list) {
                                                    if (P_CD == list[prop].C_CD) {
                                                        T_CD = list[prop].T_CD;
                                                        P_CD = list[prop].P_CD;
                                                        result[d1] = list[prop];
                                                        break
                                                    }
                                                }
                                            }
                                            break
                                        }
                                    }
                                }
                                break
                            }
                        }
                    }
                    break
                }
            }
        }
        return result
    },
    getChildrenList: function(key, tar) {
        var result = [];
        var list = eval('depth' + tar);
        for (var prop in list) {
            if (key == list[prop].P_CD) {
                result.push(list[prop])
            }
        }
        return result
    },
    getChildrenInfo: function(key) {
        var target = _BT.CategSearch.getTargetInfo(key);
        var result = _BT.CategSearch.getChildrenList(key, eval(target.T_CD) + 1);
        return result
    },
    makeMenu: function() {
        var dept1 = _BT.CategSearch.getTargetDepth(1);
        for (var x = 0; x < dept1.length; x++) {
            var cCd1 = dept1[x].C_CD;
            var lastClass = '';
            var dept2 = _BT.CategSearch.getChildrenList(cCd1, 2);
            var contents = '';
            for (var y = 0; y < dept2.length; y++) {
                if (y == (dept2.length - 1)) lastClass = 'bd_bottom01';
                else lastClass = '';
                contents += '<tr>';
                contents += '<th class="bd_left01 ' + lastClass + '">';
                contents += '<a href="'+ctx+'/shop/search/cTeg?cTp=2&cCode=' + dept2[y].C_CD + '" class="bullet_Type">';
                contents += dept2[y].C_NM;
                contents += '</a>';
                contents += '</th>';
                contents += '<td class="' + lastClass + '">';
                var dept3 = _BT.CategSearch.getChildrenList(dept2[y].C_CD, 3);
                for (var z = 0; z < dept3.length; z++) {
                    contents += '<a href="'+ctx+'/shop/search/cTeg?cTp=3&cCode=' + dept3[z].C_CD + '">' + dept3[z].C_NM + '</a>'
                }
                contents += '</td>';
                contents += '</tr>'
            }
            document.getElementById(cCd1).innerHTML = contents
        }
    },
    makeMobileMenu: function() {
        var dept1 = _BT.CategSearch.getTargetDepth(1);
        for (var x = 0; x < dept1.length; x++) {
            var cCd1 = dept1[x].C_CD;
            var lastClass = '';
            var dept2 = _BT.CategSearch.getChildrenList(cCd1, 2);
            var contents = '<a href="javascript:;" data-type="1" data-value="' + dept1[x].C_CD + '" >' + dept1[x].C_NM + '</a>';
            contents += '<ul>';
            for (var y = 0; y < dept2.length; y++) {
                contents += '<li class="depth_02_off c2 vOff ' + dept1[x].C_CD + '" data-type="2" data-type="' + dept1[x].C_CD + '" id="' + dept2[y].C_CD + '" onclick="ch(this);"><a href="javascript:;" >';
