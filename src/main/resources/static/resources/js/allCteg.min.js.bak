                }
                contents += '</td>';
                contents += '</tr>'
            }
            document.getElementById("c_" + cCd1).innerHTML = contents
        }
    },
    makeSelect: function(sel, code) {
        var obj;
        if (sel == 'Pm') {
            obj = this.getTargetDepth('1')
        } else if (code == undefined) {
            obj = this.getTargetDepth(sel)
        } else {
            obj = this.getChildrenList(code, sel)
        }
        $('#ca' + sel).empty();
        $('#ca' + sel).append('<option value="C9999">미지정</option>');
        for (var i = 0; i < obj.length; i++) {
            $('#ca' + sel).append('<option value="' + obj[i].C_CD + '">' + obj[i].C_NM + '</option>')
        }
    },
    clearSelect: function(tar) {
        for (var i = tar; i < 6; i++) {
            $('#ca' + i).empty();
            $('#ca' + i).append('<option value="C9999">미지정</option>')
        }
    }
